{
  "name": "kanastra-challenge-boilerplate",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently --kill-others \"bun run tailwindcss --watch\" \"vite\"",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "tailwindcss": "tailwindcss -i ./src/global.css -o ./dist/index.css",
    "build": "tsc && vite build && bun run tailwindcss",
    "preview": "vite preview",
    "dev:node": "concurrently \"npx tailwindcss -i ./src/global.css -o ./dist/index.css --watch\" \"vite\"",
    "build:node": "tsc && vite build && npm run tailwindcss",
    "test": "./node_modules/.bin/jest jest --config=jest.config.js"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.21.4",
    "autoprefixer": "^10.4.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.8.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^29.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nock": "^13.5.3",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^4.3.2"
  }
}
